{
  "name": "nodegui-parcel-example",
  "version": "1.0.0",
  "description": "",
  "main": "dist/src/index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "jest",
    "start": "qode dist/index.js",
    "build:typescript": "tsc",
    "build": "parcel build --target node src/index.tsx --out-dir dist",
    "watch:typescript": "tsc -w",
    "watch": "parcel watch --target node src/index.tsx --out-dir dist",
    "lint": "tslint --project tsconfig.json",
    "posttest": "npm run lint"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^24.0.18",
    "@types/node": "^12.7.3",
    "@types/react": "^16.9.2",
    "jest": "^24.9.0",
    "node-sass": "^4.12.0",
    "parcel-bundler": "^1.12.3",
    "parcel-plugin-nodegui": "^1.0.2",
    "ts-jest": "^24.0.2",
    "tslint": "^5.19.0",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "^3.6.2"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)$": "ts-jest"
    },
    "roots": [
      "src",
      "test"
    ],
    "testEnvironment": "jsdom",
    "testRegex": "^.+\\.spec\\.(ts|tsx|js)?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  },
  "dependencies": {
    "@nodegui/nodegui": "^0.1.7",
    "@nodegui/react-nodegui": "^0.1.5",
    "react": "^16.9.0"
  }
}